<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_apply_for"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cheddd.activity.ApplyForActivity">

    <com.cheddd.view.TopNavigationBar xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/tnb_applyfor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:navigation_title="@string/app_applyfor" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout style="@style/InfoRelativeLayout">

                <TextView
                    style="@style/InfoTextView"
                    android:text="续贷周期" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>
