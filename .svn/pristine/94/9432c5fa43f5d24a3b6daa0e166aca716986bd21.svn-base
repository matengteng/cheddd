<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopNavigationBar">
        <attr name="navigation_title" format="string" />
        <attr name="navigation_back" format="reference" />
    </declare-styleable>
    <declare-styleable name="DateTimeView">
        <attr name="startYearValue" format="integer" />
        <attr name="endYearValue" format="integer" />
        <attr name="minDateValue" format="string" />
        <attr name="maxDateValue" format="string" />
        <attr name="showDateTimeView">
            <flag name="dateView" value="0x0001" />
            <flag name="timeView" value="0x0004" />
            <flag name="dateTimeView" value="0x0005" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CircleProgressBar">
        <attr name="outCircleColor" format="color"></attr>
        <attr name="inCircleColor" format="color"></attr>
        <attr name="progressColor" format="color"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="textBold" format="boolean"></attr>
        <attr name="lineWidth" format="dimension"></attr>
    </declare-styleable>
</resources>